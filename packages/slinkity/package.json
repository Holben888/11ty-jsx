{
  "name": "slinkity",
  "description": "To 11ty and beyond! The all-in-one tool for templates where you want them, component frameworks where you need them",
  "version": "1.0.0-canary.0",
  "license": "MIT",
  "main": "index.cjs",
  "types": "index.d.ts",
  "exports": {
    ".": "./index.cjs",
    "./client/*": "./client/*.mjs"
  },
  "homepage": "https://slinkity.dev",
  "author": {
    "name": "Ben Holmes",
    "email": "hey@bholmes.dev",
    "url": "https://bholmes.dev/"
  },
  "engines": {
    "node": ">=14"
  },
  "keywords": [
    "static-site-generator",
    "static-site",
    "ssg",
    "documentation",
    "website",
    "jekyll",
    "blog",
    "templates",
    "generator",
    "framework",
    "eleventy",
    "eleventy-plugin",
    "11ty",
    "html",
    "markdown",
    "liquid",
    "nunjucks",
    "pug",
    "handlebars",
    "mustache",
    "ejs",
    "haml",
    "react",
    "vite",
    "bundler"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/slinkity/slinkity.git"
  },
  "bugs": "https://github.com/slinkity/slinkity/issues",
  "dependencies": {
    "devalue": "^2.0.1",
    "fs-extra": "^10.1.0",
    "nanoid": "3.3.4",
    "vite": "^3.2.4",
    "zod": "^3.18.0"
  },
  "peerDependencies": {
    "@11ty/eleventy": "^2.0.0-canary.15"
  },
  "devDependencies": {
    "@11ty/eleventy": "2.0.0-canary.15",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^18.7.23",
    "esbuild": "^0.12.29",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "faker": "^5.5.3",
    "fast-glob": "^3.2.12",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "typescript": "^4.5.5"
  },
  "scripts": {
    "build:src": "esbuild ./src/*.cts --outdir=dist --loader:.cts=ts --out-extension:.js=.cjs --format=cjs",
    "build:types": "tsc",
    "build": "run-p build:*",
    "test:e2e": "cd e2e && playwright test",
    "dev": "run-s \"build:src --watch\"",
    "play": "cd playground && eleventy --serve --incremental",
    "lint": "eslint \"*.{js,jsx,mjs}\"",
    "lint:fix": "eslint --fix \"*.{js,jsx,mjs}\""
  },
  "volta": {
    "node": "14.18.1"
  }
}
